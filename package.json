{
  "name": "enfocussimplesnippets",
  "displayName": "EnfocusSimpleSnippets",
  "description": "This Extension helps the user get quick code snippets commonly used when creating custom scripts",
  "version": "0.0.5",
  "publisher": "ruffWebDev",
  "author": {
		"name": "Tom Ruff",
		"email": "tom@idsignco.com",
		"url": "https://github.com/Ruffmantom"
	},
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
		"enfocus scripting",
		"enfocus",
		"switch automation",
		"enfocus automation",
		"automation"
	],
  "license": "MIT",
	"icon": "assets/images/icon-min.png",
  "contributes": {
    "commands": [
      {
        "command": "enfocussimplesnippets.helloWorld",
        "title": "Hello World"
      }
    ],
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/enfocus.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/enfocus.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/enfocus.code-snippets"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/enfocus.code-snippets"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "repository": {
		"type": "git",
		"url": "https://github.com/Ruffmantom/Enfocus-Switch-Script-Snippets-Extension.git"
	},
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.21.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
